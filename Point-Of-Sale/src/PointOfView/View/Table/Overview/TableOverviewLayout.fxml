      <Label fx:id="tableNumber" alignment="CENTER" text="TableNumberLabel" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets left="5.0" top="2.0" />
         </BorderPane.margin>
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </top>
   <bottom>
      <Label fx:id="tableSumPrice" text="TablePriceLabel" BorderPane.alignment="CENTER_RIGHT">
         <BorderPane.margin>
            <Insets bottom="2.0" right="5.0" />
         </BorderPane.margin>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </bottom>
   <center>
      <Label fx:id="tableMenuList" text="Order Resipe List" BorderPane.alignment="TOP_LEFT">
         <BorderPane.margin>
            <Insets left="5.0" top="2.0" />
         </BorderPane.margin>
